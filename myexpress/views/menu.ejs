<!DOCTYPE html>
<html>
  <head>
    <title>商品</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>商品列表</h1>
    <table>
		<thead>
			<tr>
				<th>商品</th>
				<th>价格</th>
				<th>图片</th>
				<th>编辑</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0;i<goodsList.length;i++){%>
			<tr>
				<td><%= goodsList[i].goodsname%></td>
				<td><%= goodsList[i].price%></td>
				<td><img src="<%= goodsList[i].img_url%>"></td>
				<td><a href="/menu/edit?id=goodsList[i]._id"></a></td>
			</tr>
			<%}%>
		</tbody>
    </table>
   
  </body>
</html>
